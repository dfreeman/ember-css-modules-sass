{
  "name": "ember-css-modules-sass",
  "version": "1.1.0",
  "description": "An ember-css-modules plugin that configures the build pipeline for use with Sass",
  "keywords": [
    "ember-addon",
    "ember-css-modules-plugin"
  ],
  "repository": "dfreeman/ember-css-modules-sass",
  "license": "MIT",
  "author": "Dan Freeman",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
